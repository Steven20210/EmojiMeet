{
  "name": "EmojiMeet",
  "description": "An extension to display emotions as a background in Google Meet",
  "permissions": ["activeTab", "storage", "tabs", "notifications", "debugger"],
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["src/script.js"]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}
